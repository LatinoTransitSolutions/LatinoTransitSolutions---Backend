{
    "compilerOptions": {
        "target": "ES6",
        "module": "CommonJS",
        "moduleResolution": "Node",
        "allowImportingTsExtensions": true,
        "outDir": "./dist",
        "noEmit": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "noImplicitAny": true,
        "preserveConstEnums": true,
        "sourceMap": true,
    }
}
